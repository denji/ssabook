@conference{Hack06b,
  title =	 {{R}egister {A}llocation for {P}rograms in
                  {SSA}-{F}orm},
  booktitle =	 {Compiler Construction 2006},
  year =	 {2006},
  month =	 {March},
  publisher =	 {Springer},
  volume =	 {3923},
  pages =	 {247--262},
  journal =	 {Lecture Notes In Computer Science},
  abstract =	 {As register allocation is one of the most important
                  phases in optimizing compilers, much work has been
                  done to improve its quality and speed.  We present a
                  novel register allocation architecture for programs
                  in SSA-form which simplifies register allocation
                  significantly. We investigate certain properties of
                  SSA-programs and their interference graphs, showing
                  that they belong to the class of chordal
                  graphs. This leads to a quadratic-time optimal
                  coloring algorithm and allows for decoupling the
                  tasks of coloring, spilling and coalescing
                  completely. After presenting heuristic methods for
                  spilling and coalescing, we compare our coalescing
                  heuristic to an optimal method based on integer
                  linear programming.},
  author =	 {Sebastian Hack and Daniel Grund and Gerhard Goos},
  editor =	 {Andreas Zeller and Alan Mycroft},
  doi =		 {10.1007/11688839\_20},
}

