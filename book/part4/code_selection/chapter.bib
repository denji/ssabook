@techreport{Hack05,
  title =	 {{T}owards {R}egister {A}llocation for {P}rograms in
                  {SSA}-{F}orm},
  year =	 {2005},
  month =	 {September},
  number =	 {2005-27},
  abstract =	 {In this technical report, we present an architecture
                  for register allocation on the SSA form. We show,
                  how the properties of SSA-form programs and their
                  interference graphs can be exploited to develop new
